<div class='row'>
	<div class='col-ex-12'>
		<%= form_for(obj, :html => {class: "form-horizontal", role: :"form"}) do |f| %>

		<div class="container">

			<div class="well well-lg">

				<div class="col-sm-6">
					<div class="form-group">
							<label for="field_1">Project name</label>
						<div class="textfield">
							<%= f.text_field :field_1, class: "form-control", placeholder: "Field 1" %>
						</div>

					</div>
			
				
					<div class="form-group">

						<label for="field_2">Company registration number</label>

						<div class="textfield">
							<%= f.text_field :field_2, class: "form-control", placeholder: "Field 2" %>
						</div>

					</div>

					<div class="form-group">
						<label for="field_3">Some other info</label>
							<div class="textfield">
								<%= f.text_field :field_3, class: "form-control", placeholder: "Field 3" %>
							</div>

					</div>
				</div>

				<div class="col-sm-6"> 
					<div class="form-group">
						<label for="field_4">Some other info</label>
							<div class="textfield">
								<%= f.text_field :field_4, class: "form-control", placeholder: "Field 4" %>
							</div>

					</div>

					<div class="form-group">
						<label for="field_5">Some other info</label>
							<div class="textfield">
								<%= f.text_field :field_5, class: "form-control", placeholder: "Field 5" %>
							</div>

					</div>

					<div class="form-group">
						<label for="field_6">Some other info</label>
							<div class="textfield">
								<%= f.text_field :field_6, class: "form-control", placeholder: "Field 6" %>
							</div>

					</div>
		 	 </div>

			  <div class="form-inline">
   				<%= f.check_box :accept_tnc, class: 'form-control' %>
   				<%= f.label :subscribed, "I have read and understand the Terms of Services" %>
				</div>


				<div class="form-group">
					<div class="col-sm-offset-5 col-sm-10">
						<%= f.submit class: 'btn btn-primary btn-lg' %>
					</div>
				</div>

			</div>

		</div>


		<% end %>

	</div>
</div>